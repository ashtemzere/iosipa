        function downloadCount(app_id) {

            $.ajax({
                type: "PUT",
                url: "https://www.installonair.com/app-download-count",
                data: { app_id : app_id },
                headers: {
                    "X-CSRF-TOKEN": "1jz2idxUwwSyTs6JpPWjLQAldnwReVkaLoUs6aSJ"
                },
                success: function (data) {
